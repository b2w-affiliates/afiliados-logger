{
    "extends": "airbnb-base",
    "rules": {
        "arrow-body-style": 0,
        "arrow-parens": [2, "as-needed"],
        "no-use-before-define": 0,
        "func-names": 0,
        "global-require": 0,
        "no-param-reassign": 0,
        "comma-dangle": [2, {
          "functions": "never",
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline"
        }]
    }
}
